{
  "name": "Injecta",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Injecting stuff",
  "homepage_url": "http://danharper.me",
  "content_scripts" : [{
    "matches": ["<all_urls>"],
    "js" : [
      "jquery.js","tipped.js","numbers.js"],
      "css" : [
        "numbers.css","tipped.css"
      ]
    }],
  "web_accessible_resources":["numbers.js"],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}